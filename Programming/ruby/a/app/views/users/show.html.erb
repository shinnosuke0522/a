<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @user.name %>
</p>

<p>
  <strong>Email:</strong>
  <%= @user.email %>
</p>

<div class="friend list">
  <strong>Friend List </strong>
  <ul>
    <% @user.friends.each do |friend| %>
      <li><%=friend.name%></li>
    <% end %>
  </ul>
</div>

<p>
<%= link_to 'Edit', edit_user_path(@user) %> |
<%= link_to 'Back', users_path %>
</p>